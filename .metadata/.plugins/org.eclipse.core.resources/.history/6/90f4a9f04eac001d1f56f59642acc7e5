package buoi2;

import java.util.Scanner;

public class Diem {
	private int x;
	private int y;
	public Diem() {
		x = 0;
		y = 0;
	}
	public Diem(int x1, int y1) {
		x = x1;
		y = y1;
	}
	private void gan(int x1, int y1) {
		x = x1;
		y = y1;
	}
	private void in() {
		System.out.println("("+x+","+y+")");
	}
	private void nhap() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Nhap tu ban phim x");
		x = sc.nextInt();
		
	}
}
